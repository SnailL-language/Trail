fn factorial(n: usize) -> usize {
    if (n == 0 || n == 1) {
        return 1;
    }
    return n * factorial(n - 1);
}


fn main() {
    let a: usize = 5;
    let v: [i32; 3] = [0, 1, 2];
    for (let i: i32 = 0;  i < 3; i += 1) {
        let b: i32 = factorial(i);
        if (b > 1000) {
            break;
        }
    }
    let c: i32 = 1;
    let d: i32 = 2;
    while (c < 50 && d == 100) {
        c /= d;
    }
    let g: string = "fagfsg";
}