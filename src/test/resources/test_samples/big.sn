fn factorial(n: usize) -> usize {
    if (n == 0 || n == 1) {
        return 1;
    }
    return n * factorial(n - 1);
}


fn main() {
    for (let i: usize = 0;  i < 3; i += 1) {
        let b: usize = factorial(i);
        if (b > 1000) {
            break;
        }
    }
    let c: i32 = 1;
    let d: i32 = 2;
    while (c < 50 && d == 100) {
        c /= d;
    }
}